<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.WeatherController">
    <TextField fx:id="locationField" />
    <Button fx:id="searchButton" text="Search" onAction="#searchWeather" />
    <Label fx:id="locationLabel" />
    <Label fx:id="weatherLabel" />
</VBox>